{
  "name": "ecommerce-frangauna01",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "fix-imports": "npx ts-node src/modules/utils/scripts/fix-imports.script.ts",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --config ./src/configs/jest.config.ts",
    "test:watch": "jest --watch --config ./src/configs/jest.config.ts",
    "test:cov": "jest --coverage --config ./src/configs/jest.config.ts",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand --config ./src/configs/jest.config.ts",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli",
    "migration:run": "npm run typeorm -- migration:run -d ./src/configs/typeorm.config.ts",
    "migration:run:prod": "npx typeorm migration:run -d dist/configs/typeorm.config.js",
    "migration:generate": "npm run typeorm -- migration:generate ./src/migrations/Init -d ./src/configs/typeorm.config.ts",
    "migration:create": "npm run typeorm -- migration:create ./src/migrations/Manual",
    "migration:revert": "npm run typeorm -- migration:revert -d ./src/configs/typeorm.config.ts",
    "migration:show": "npm run typeorm -- migration:show -d ./src/configs/typeorm.config.ts",
    "docker:up": "docker-compose up -d --build",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f --tail=100",
    "docker:restart": "docker-compose restart"
  },
  "dependencies": {
    "@nestjs/common": "^11.0.1",
    "@nestjs/core": "^11.0.1",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^11.0.1",
    "bcryptjs": "^3.0.2",
    "buffer-to-stream": "^1.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cloudinary": "^2.7.0",
    "express-openid-connect": "^2.18.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/config": "^4.0.2",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/testing": "^11.0.1",
    "@nestjs/typeorm": "^11.0.0",
    "@stylistic/eslint-plugin": "^5.1.0",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.10.7",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "pg": "^8.16.3",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-morph": "^26.0.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typeorm": "^0.3.25",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  }
}
